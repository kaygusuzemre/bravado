{
  "name": "bravado",
  "version": "1.0.0",
  "description": "habit-tracker and challenge manager in social way",
  "author": "Davut KARA, Ahmet Dogukan Citak, Emre KAYGUSUZ, Efe Ali Gulocak",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development  nodemon -r esm server/index.js --watch server",
    "dev2": "cross-env NUXT_HOST=`my-local-ip` NUXT_PORT=3000 NODE_ENV=development nodemon -r esm server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest --coverage --collectCoverageFrom=test/**/*.test.js",
    "fix": "npm run lint -- --fix",
    "setup": "node ./server/setup.js",
    "docker": "docker-compose up -d"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.21",
    "chance": "^1.0.18",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "defined": "^1.0.0",
    "express": "^4.17.0",
    "express-session": "^1.16.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mysql": "^2.17.1",
    "nuxt": "^2.4.0",
    "p5": "^0.8.0",
    "rand-token": "^0.4.0",
    "redis": "^2.8.0",
    "session": "^0.1.0",
    "undefsafe": "^2.0.2",
    "validator": "^10.11.0",
    "vue-color-picker-wheel": "^0.4.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "my-local-ip": "^1.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "vue-jest": "^3.0.3"
  }
}
